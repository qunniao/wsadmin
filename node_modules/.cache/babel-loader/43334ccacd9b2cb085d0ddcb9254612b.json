            roleName :this.dataForm.roleName,\n              note:this.dataForm.note        \n            }\n            if(this.updateId==0){\n             \n              pushAdminRoleInfoAPI(params)\n                .then(res => {\n                    const sucresponse = res.data;\n                    if(sucresponse.code==200){\n                      this.$message({\n                        type: 'success',\n                        message: '添加成功',\n                      });\n                      this.$emit('close-form')\n                      this.$emit('finish-action')\n                    }else{\n                      this.errorMsg(sucresponse.message)\n                    }\n                })\n                .catch(err => {\n                  console.log(err)\n                    this.tableLoading = false;\n                })\n            }else{\n              params.id=this.updateId\n              putAdminRoleInfoAPI(params)\n                .then(res => {\n                    const sucresponse = res.data;\n                    if(sucresponse.code==200){\n                      this.$message({\n                        type: 'success',\n                        message: '修改成功',\n                      });\n                      this.$emit('close-form')\n                      this.$emit('finish-action')\n                    }else{\n                      this.errorMsg(sucresponse.message)\n                    }\n                })\n                .catch(err => {\n                  console.log(err)\n                    this.tableLoading = false;\n                })\n            }\n\n            console.log(params)\n          }\n        })\n    },\n    \n    getDetail(id){\n          const params =id\n            getAdminRoleInfoAPI(params)\n                .then(res => {\n                    console.log(res)\n                    const data=res.data.data\n                    this.dataForm=data\n                })\n                .catch(err => {\n                    this.tableLoading = false;\n                })\n    },\n    errorMsg (message) {\n      this.$message({\n        message: message,\n        type: 'error',\n        duration: 1500\n      })\n    },\n    /*\n      * 返回上层\n      */\n    backhtmls(){\n        this.$emit('close-form')\n    }    \n  },\n  mounted() {\n    if(this.updateId!=0){\n      this.getDetail(this.updateId)\n    }\n      console.log(this.updateId)\n  },\n}\n</script>\n<style scoped=\"scoped\" lang=\"less\">\n.mbline{\n  margin-top:24px;\n  padding-left: 20px;\n  line-height: 40px;\n  background: rgb(249, 249, 249);\n}\n.avatar{\n  width: 100px;\n  height: 100px;\n}\n</style>"],"sourceRoot":"src\\components\\adminRole"}]}