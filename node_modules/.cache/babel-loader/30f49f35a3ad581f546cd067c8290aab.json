model=\"ommodityType\" clearable placeholder=\"请选择库存类型\" >\n                            <el-option\n                            :value=\"1\"\n                            label=\"电商\"\n                            ></el-option>\n                            <el-option\n                            :value=\"0\"\n                            label=\"app\"\n                            ></el-option>\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"变更项：\">\n                        <el-select v-model=\"ommodityType\" clearable placeholder=\"请选择变更项\" >\n                            <el-option\n                            :value=\"1\"\n                            label=\"电商\"\n                            ></el-option>\n                            <el-option\n                            :value=\"0\"\n                            label=\"app\"\n                            ></el-option>\n                        </el-select>\n                    </el-form-item>\n                    <el-form-item label=\"选择日期：\">\n                        <el-date-picker\n                        v-model=\"value1\"\n                        type=\"datetimerange\"\n                        range-separator=\"至\"\n                        start-placeholder=\"开始日期\"\n                        end-placeholder=\"结束日期\">\n                        </el-date-picker>\n                    </el-form-item>\n                    <el-form-item label=\"订单类型\">\n                        <el-select v-model=\"ommodityType\" clearable placeholder=\"请选择订单类型\" >\n                            <el-option\n                            :value=\"1\"\n                            label=\"电商\"\n                            ></el-option>\n                            <el-option\n                            :value=\"0\"\n                            label=\"app\"\n                            ></el-option>\n                        </el-select>\n                    </el-form-item>\n