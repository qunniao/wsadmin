eforeAvatarUpload\">\r\n            <img v-if=\"dataForm.cover\" :src=\"dataForm.cover\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n        <el-form-item  label=\"组合套餐名称：\">\r\n         <el-input\r\n            type=\"text\"\r\n            placeholder=\"请输入组合套餐名称\"\r\n            v-model=\"text\"\r\n            maxlength=\"10\"\r\n            show-word-limit\r\n            >\r\n         </el-input>\r\n      </el-form-item>\r\n      <el-form-item label='组合套餐运费:'>\r\n            <el-radio v-model=\"radio\" label=\"1\">包邮</el-radio>\r\n            <el-radio v-model=\"radio\" label=\"2\">按默认设置</el-radio>\r\n            <el-radio v-model=\"radio\" label=\"3\">自定义</el-radio>\r\n      </el-form-item>\r\n      <el-form-item label='自定义折扣:'>\r\n            <el-radio v-model=\"radio\" label=\"1\">默认折扣</el-radio>\r\n            <el-radio v-model=\"radio\" label=\"2\">自定义</el-radio>\r\n      </el-form-item>\r\n        <el-form-item>\r\n            <el-button type=\"primary\">选择商品</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n            <el-table\r\n            :data=\"tableData\"\r\n            border\r\n            size=\"medium \"\r\n            style=\"width: 100%\">\r\n                <el-table-column\r\n                prop=\"departmentName\"\r\n                label=\"商品信息\"\r\n                min-width=\"360\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                prop=\"price\"\r\n                label=\"状态\"\r\n                sortable='custom'\r\n                min-width=\"180\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                prop=\"subordinateDepartment\"\r\n                label=\"库存\"\r\n                min-width=\"180\">\r\n                </el-table-column>\r\n                <el-table-column\r\n                prop=\"subordinateDepartment\"\r\n             